#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kng/proj/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/apache-arrow@11.0.0/node_modules/apache-arrow/bin/node_modules:/Users/kng/proj/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/apache-arrow@11.0.0/node_modules/apache-arrow/node_modules:/Users/kng/proj/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/apache-arrow@11.0.0/node_modules:/Users/kng/proj/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kng/proj/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/apache-arrow@11.0.0/node_modules/apache-arrow/bin/node_modules:/Users/kng/proj/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/apache-arrow@11.0.0/node_modules/apache-arrow/node_modules:/Users/kng/proj/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/apache-arrow@11.0.0/node_modules:/Users/kng/proj/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/arrow2csv.js" "$@"
else
  exec node  "$basedir/../../bin/arrow2csv.js" "$@"
fi
