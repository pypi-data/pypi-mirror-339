#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kng/gh/st-components/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/esbuild@0.25.2/node_modules/esbuild/bin/node_modules:/Users/kng/gh/st-components/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/esbuild@0.25.2/node_modules/esbuild/node_modules:/Users/kng/gh/st-components/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/esbuild@0.25.2/node_modules:/Users/kng/gh/st-components/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kng/gh/st-components/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/esbuild@0.25.2/node_modules/esbuild/bin/node_modules:/Users/kng/gh/st-components/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/esbuild@0.25.2/node_modules/esbuild/node_modules:/Users/kng/gh/st-components/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/esbuild@0.25.2/node_modules:/Users/kng/gh/st-components/st-dialog-close-detector/src/st_dialog_close_detector/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../../../esbuild/bin/esbuild" "$@"
fi
