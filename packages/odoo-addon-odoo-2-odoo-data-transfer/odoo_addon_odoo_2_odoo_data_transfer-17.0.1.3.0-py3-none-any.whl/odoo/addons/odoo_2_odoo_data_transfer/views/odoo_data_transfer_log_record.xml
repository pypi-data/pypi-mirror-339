<odoo>

     <record id="odoo_data_transfer_log_record_tree" model="ir.ui.view">
          <field name="name">odoo.data.transfer.log.record.tree</field>
          <field name="model">odoo.data.transfer.log.record</field>
          <field name="arch" type="xml">
               <tree create="false">
                    <field name="local_id" widget="reference" />
                    <field name="remote_id" />
                    <field name="error_msg" />
               </tree>
          </field>
     </record>

     <record id="odoo_data_transfer_log_record_form" model="ir.ui.view">
          <field name="name">odoo.data.transfer.log.record.form</field>
          <field name="model">odoo.data.transfer.log.record</field>
          <field name="arch" type="xml">
               <form create="false" edit="false">
                    <group>
                         <field name="local_id" widget="reference" />
                         <field name="remote_id" />
                    </group>
                    <group invisible="not error_type">
                         <field name="error_type" invisible="1" />
                         <field name="error_msg" />
                    </group>
               </form>
          </field>
     </record>

</odoo>
