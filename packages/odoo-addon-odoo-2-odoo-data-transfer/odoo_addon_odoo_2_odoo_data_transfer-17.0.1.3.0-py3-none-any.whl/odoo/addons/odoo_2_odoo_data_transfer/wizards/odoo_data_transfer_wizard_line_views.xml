<odoo>

     <record id="odoo_data_transfer_wizard_line_view_form" model="ir.ui.view">
          <field name="name">odoo.data.transfer.template.line.view.form.inherit</field>
          <field name="model">odoo.data.transfer.wizard.line</field>
          <field
            name="inherit_id"
            ref="odoo_2_odoo_data_transfer.odoo_data_transfer_template_line_mixin_view_form"
        />
          <field name="mode">primary</field>
          <field name="arch" type="xml">
               <field name="model" position="after">
                    <field name="transfer_id" invisible="1" />
               </field>
               <field name="one2many_template_id" position="attributes">
                    <attribute name="readonly">True</attribute>
               </field>
          </field>
     </record>

     <record id="odoo_data_transfer_wizard_line_view_tree" model="ir.ui.view">
          <field name="name">odoo.data.transfer.template.line.view.tree.inherit</field>
          <field name="model">odoo.data.transfer.wizard.line</field>
          <field
            name="inherit_id"
            ref="odoo_2_odoo_data_transfer.odoo_data_transfer_template_line_mixin_view_tree"
        />
          <field name="mode">primary</field>
          <field name="arch" type="xml">
               <field name="remote_source_field" position="after">
                    <field name="transfer_id" column_invisible="1" />
               </field>
          </field>
     </record>

</odoo>
