# YouTrack MCP Server - Environment Variables
# Rename this file to .env and fill in your YouTrack credentials

# YouTrack instance URL (e.g., https://yourcompany.youtrack.cloud)
YOUTRACK_URL=

# YouTrack API permanent token
YOUTRACK_TOKEN=

# MCP server settings
MCP_SERVER_NAME=YouTrack MCP Server
MCP_LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR
