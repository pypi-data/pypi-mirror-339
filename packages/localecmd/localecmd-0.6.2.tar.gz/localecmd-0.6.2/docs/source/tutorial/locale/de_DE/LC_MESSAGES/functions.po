#: ../source/functions.md:1
msgid "Module functions"
msgstr "Turtle-Funktionen"

#: ../source/functions.md:6
msgid "Stop recording, close the turtle window, and exit:  BYE"
msgstr "Aufnahme beenden, Turtle-Fenter schließen und Programm schließen: TSCHÜSS"

#: ../source/functions.md:11
msgid "Draw circle with given radius an options extent and steps:  CIRCLE 50"
msgstr "Kreis mit gegebenem Radius zeichnen: KREIS 50"

#: ../source/functions.md:16
msgid "Move the turtle forward by the specified distance:  FORWARD 10"
msgstr "Schildkröte vorwärts bewegen:  VORWÄRTS 10"

#: ../source/functions.md:21
msgid ""
"Move turtle to an absolute position with changing orientation.  GOTO 100 "
"200"
msgstr "Schildkröte gedreht an bestimmte Stelle bewegen.  GEH_NACH 100 "

#: ../source/functions.md:26
msgid "Print the current turtle heading in degrees:  HEADING"
msgstr "Ausrichtung der Schildkröte drucken:  RICHTUNG"

#: ../source/functions.md:31
msgid "Return turtle to the home position:  HOME"
msgstr "Schildkröte nach Hause ziehen:  NACH_HAUSE"

#: ../source/functions.md:36
msgid "Turn turtle left by given number of degrees:  LEFT 90"
msgstr "Schildkröte um einen Winkel in Grad nach links drehen:  LINKS 90"

#: ../source/functions.md:41
msgid "Rerun all commands between two line numbers"
msgstr "Alle Befehle zwichen zwei Zeilen wieder ausführen"

#: ../source/functions.md:46
msgid "Print the current turtle position: POSITION"
msgstr "Jetzigen Ort der Schildkröte drucken:  POSITION"

#: ../source/functions.md:51
msgid "Clear the screen and return turtle to center:  RESET"
msgstr "Fenster entleeren und Schildkröte in die Mitte setzen:  ZURÜCKSETZEN"

#: ../source/functions.md:56
msgid "Turn turtle right by given number of degrees:  RIGHT 20"
msgstr "Schildkröte um einen Winkel in Grad nach rechts drehen:  RECHTS 90"

