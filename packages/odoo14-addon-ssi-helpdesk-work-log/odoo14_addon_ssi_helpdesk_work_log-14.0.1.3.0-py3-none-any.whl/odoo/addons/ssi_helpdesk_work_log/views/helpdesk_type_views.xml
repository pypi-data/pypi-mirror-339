<odoo>
<record id="helpdesk_type_view_form" model="ir.ui.view">
    <field name="name">helpdesk_type - form</field>
    <field name="model">helpdesk_type</field>
    <field name="inherit_id" ref="ssi_helpdesk.helpdesk_type_view_form" />
    <field name="arch" type="xml">
        <data>
            <xpath expr="//page[last()]" position="before">
                <page name="work_log" string="Work Log">
                    <group name="work_log_1" colspan="4" col="2">
                        <group name="work_log_1_1" colspan="1" col="2">
                            <field name="work_estimation" widget="float_time" />
                        </group>
                        <group name="work_log_1_2" colspan="1" col="2">
                            <field
                                    name="work_log_analytic_account_ids"
                                    widget="many2many_tags"
                                />
                            <field
                                    name="work_log_analytic_group_ids"
                                    widget="many2many_tags"
                                />
                        </group>
                    </group>
                </page>
            </xpath>
        </data>
    </field>
</record>
</odoo>
