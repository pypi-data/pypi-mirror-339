# source .env
# Important: Make sure there are no spaces in this file to run the source command

# netrc file is for authentication to the `check-url`, to protect against accidental version overwrites during publishing
NETRC=./.netrc
# publish vars are for authentication to the `publish-url` for the actual uploads
UV_PUBLISH_USERNAME=artifactory_username
UV_PUBLISH_PASSWORD=token_generated_in_artifactory
