### Core auth values

FORDPASS_USERNAME=
FORDPASS_PASSWORD=
FORDPASS_VIN=

# Decimal places to round floats to.
FORDPASS_DECIMAL_PLACES=2

### Default units used when converting the unit to a string.
# f = Fahrenheit, c = Celsius
VEHICLEPASS_DEFAULT_TEMP_UNIT=f

# mi = miles, km = kilometers
VEHICLEPASS_DEFAULT_DISTANCE_UNIT=mi

# psi = pounds per squre inch, kpa = kilopascals
VEHICLEPASS_DEFAULT_PRESSURE_UNIT=psi

# v = volts, mv = millivolts
VEHICLEPASS_DEFAULT_ELECTRIC_POTENTIAL_UNIT=v

# s = seconds, m = minutes, h = hours, ms = milliseconds, human_readable = human readable format (0h 2m 0s)
VEHICLEPASS_DEFAULT_TIME_UNIT=human_readable