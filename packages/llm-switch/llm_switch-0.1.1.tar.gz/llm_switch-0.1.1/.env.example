# LLM 提供商 API 密鑰
# 複製此文件為 .env 並填入您的 API 密鑰

# OpenAI
OPENAI_API_KEY=your-openai-api-key
OPENAI_API_BASE=https://api.openai.com/v1

# DeepSeek
DEEPSEEK_API_KEY=your-deepseek-api-key
DEEPSEEK_API_BASE=https://api.deepseek.com/v1

# XAI
XAI_API_KEY=your-xai-api-key
XAI_API_BASE=https://api.xai.com/v1

# Anthropic
ANTHROPIC_API_KEY=your-anthropic-api-key
ANTHROPIC_API_BASE=https://api.anthropic.com/v1

# Google (Gemini)
GOOGLE_API_KEY=your-google-api-key
GOOGLE_API_BASE=https://generativelanguage.googleapis.com/v1

# Azure OpenAI
AZURE_OPENAI_API_KEY=your-azure-openai-api-key
AZURE_OPENAI_API_BASE=https://{your-resource-name}.openai.azure.com
AZURE_OPENAI_API_VERSION=2023-05-15
AZURE_OPENAI_DEPLOYMENT_NAME=your-gpt4-deployment

# HuggingFace
HUGGINGFACE_API_KEY=your-huggingface-api-key
HUGGINGFACE_API_BASE=https://api-inference.huggingface.co/models
