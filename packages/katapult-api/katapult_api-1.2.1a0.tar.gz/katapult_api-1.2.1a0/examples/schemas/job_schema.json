{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "connections": {
      "type": "object",
      "patternProperties": {
        ".*": {
          "type": "object",
          "properties": {
            "attributes": {
              "type": "object",
              "properties": {
                "connection_type": {
                  "type": "object",
                  "properties": {
                    "button_added": {
                      "type": "string"
                    }
                  }
                }
              }
            },
            "button": {
              "type": "string"
            },
            "node_id_1": {
              "type": "string"
            },
            "node_id_2": {
              "type": "string"
            }
          },
          "required": [
            "button",
            "node_id_1",
            "node_id_2"
          ]
        }
      }
    },
    "date_created": {
      "type": "number"
    },
    "geohash": {
      "type": "object"
    },
    "job_auth_token": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          },
          "valid": {
            "type": "boolean"
          }
        }
      }
    },
    "job_creator": {
      "type": "string"
    },
    "job_owner": {
      "type": "string"
    },
    "map_styles": {
      "type": "object"
    }
  },
  "required": [
    "date_created",
    "job_creator",
    "job_owner"
  ]
}
