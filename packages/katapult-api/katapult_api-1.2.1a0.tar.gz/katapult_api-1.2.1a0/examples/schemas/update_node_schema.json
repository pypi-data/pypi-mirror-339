{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "key": {
      "type": "string"
    },
    "data": {
      "type": "object",
      "properties": {
        "_created": {
          "type": "object",
          "properties": {
            "method": {
              "type": "string"
            },
            "timestamp": {
              "type": "number"
            },
            "uid": {
              "type": "string"
            }
          },
          "required": [
            "method",
            "timestamp",
            "uid"
          ]
        },
        "attributes": {
          "type": "object",
          "patternProperties": {
            ".*": {
              "type": "object",
              "patternProperties": {
                ".*": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            }
          },
          "additionalProperties": false
        },
        "latitude": {
          "type": "number"
        },
        "longitude": {
          "type": "number"
        }
      },
      "required": [
        "_created"
      ]
    }
  },
  "required": [
    "key",
    "data"
  ]
}
