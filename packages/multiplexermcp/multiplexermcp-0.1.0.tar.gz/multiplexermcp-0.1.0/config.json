{
    "mcpServers": {
        "wolfram-only": {
            "command": "uv",
            "args": [
                "run",
                "multiplexermcp"
            ],
            "env": {
                "ENABLED_SERVERS": "[\"wolframalpha\"]"
            }
        },
        "searxng-only": {
            "command": "uv",
            "args": [
                "run",
                "multiplexermcp"
            ],
            "env": {
                "ENABLED_SERVERS": "[\"searxng\"]"
            }
        },
        "wolfram-and-searxng": {
            "command": "uv",
            "args": [
                "run",
                "multiplexermcp"
            ],
            "env": {
                "ENABLED_SERVERS": "[\"wolframalpha\", \"searxng\"]"
            }
        }
    }
}